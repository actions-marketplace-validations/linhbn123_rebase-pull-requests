{
  "name": "rebase",
  "version": "1.0.0",
  "private": true,
  "description": "Rebase pull requests in a repository",
  "main": "lib/main.js",
  "scripts": {
    "clean": "rm -rf node_modules package-lock.json",
    "build-checkout": "npm install --prefix node_modules/checkout && npm run build --prefix node_modules/checkout",
    "build": "tsc && ncc build",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "jest unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linhbn123/rebase-pull-requests.git"
  },
  "keywords": [
    "actions",
    "pull",
    "rebase"
  ],
  "author": "Linh Bui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linhbn123/rebase-pull-requests/issues"
  },
  "homepage": "https://github.com/linhbn123/rebase-pull-requests",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/io": "1.0.2",
    "@octokit/graphql": "4.5.6",
    "checkout": "github:actions/checkout#v2.2.0",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@types/jest": "26.0.14",
    "@types/node": "14.10.3",
    "@typescript-eslint/parser": "4.1.1",
    "@vercel/ncc": "0.24.1",
    "eslint": "7.9.0",
    "eslint-plugin-github": "4.1.1",
    "eslint-plugin-jest": "24.0.1",
    "jest": "26.4.2",
    "jest-circus": "26.4.2",
    "js-yaml": "3.14.0",
    "prettier": "2.1.2",
    "ts-jest": "26.3.0",
    "typescript": "4.0.2"
  }
}
